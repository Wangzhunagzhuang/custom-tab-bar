/**app.wxss**/
page {
  background: #E4E4E4;
}

page .tabbar {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: white;
  padding-bottom: 70rpx;
  box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 3px 14px 2px rgba(0, 0, 0, 0.12), 0px 8px 10px 1px rgba(0, 0, 0, 0.14);
  border-radius: 12px 12px 0px 0px;
}

page .tabbar .weui-tabbar__label {
  font-size: 24rpx;
}

.b {
  font-weight: 550;
}

.scrollbarnone::-webkit-scrollbar {
  display: none;
}