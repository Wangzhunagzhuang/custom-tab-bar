.tab-bar{
  position: fixed;
  bottom: 0px;
  left: 0px;
  right: 0px;
  width: 100%;
  height: 120rpx;
  background:white;
  display: flex;
  flex-direction: row;
  padding-bottom: env(safe-area-inset-bottom);
}
.tab-bar-item{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tab-bar-item-else{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #8C8D99;
  text-align: center;
}
.item_image{
  width: 56rpx;
  height: 56rpx;
  object-fit: cover;
}
.item_image_actuve{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: -10rpx;
  border-radius: 50%;
  border: 6rpx solid white;
  background:white;
}
.tabbar_active{
  color: #191B33;
}